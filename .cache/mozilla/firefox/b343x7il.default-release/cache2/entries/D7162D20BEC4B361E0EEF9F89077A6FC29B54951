							     if(this.key === modul.tipus){ 
							     	  nom=this.nom; 
							        nou=this.nou; 
							        act=this.act;							     } 
							}); 
							
							if (modul.estat=='updated') { $("#ma"+val.ided).append("<li><a class='desplegable-item "+modul.tipus+"' href='"+modul.url+"' target='_blank'><span class='actualitzat'>"+nom+" "+act+"</span> "+modul.nom+"</a></li>");	}
