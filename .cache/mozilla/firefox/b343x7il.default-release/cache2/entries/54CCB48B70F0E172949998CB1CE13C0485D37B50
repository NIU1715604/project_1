.boto-checkbox {
    cursor: pointer;
    position: absolute;
    z-index: 1;
    top: 1rem;
    left: 0;
    width: 3rem;
    height: 3rem;
    margin: 0;

    display: grid;
    place-content: center;

    /* Add if not using autoprefixer */
    -webkit-appearance: none;
    /* Remove most all native input styles */
    appearance: none;
    /* For iOS < 15 */
    background-color: #fff;
    /* Not removed via appearance */
}

.boto-radio {
    cursor: pointer;
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width: 4rem;
    height: 4rem;
    margin: 0;
    opacity: 0;
}

