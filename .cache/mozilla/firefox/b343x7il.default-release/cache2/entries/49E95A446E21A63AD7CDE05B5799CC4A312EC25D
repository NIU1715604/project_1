$(document)
	.ready(function () {

		//
		// Focus per teclat
		// ---------------------------------------------------------------

		$("body")
			.on("keyup", "a, button, input, textarea, select", function (n) {
				var t = n.keyCode ? n.keyCode : n.which,
					i = document.activeElement;
				t == "9" && i
					.classList
					.add("focus");
			});

		$("body")
			.on("blur", ".focus", function (n) {
				n
					.currentTarget
					.classList
					.remove("focus");
				n.currentTarget.classList.length === 0 && n
					.currentTarget
					.removeAttribute("class");
			});
	});

