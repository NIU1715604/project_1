	$('input[type="radio"]').click(function(){
		var val = $(this).attr("value");
		var target = $("." + val);
		$(".multiopcio").not(target).removeClass("on").addClass("off");
		$(target).removeClass("off").addClass("on");
	});
/*	$('.activar-aula').change(function() {
